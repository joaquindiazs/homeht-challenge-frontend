lues }\n                            />\n                    </div>\n                )\n            case 2:\n                return (\n                    <div>\n                        <LinearProgress style={styles.progressBar} variant=\"determinate\" value={step * 100 / 3} />\n                        <Confirm\n                            nextStep = { this.nextStep }\n                            prevStep = { this.prevStep }\n                            handleChange = { this.handleChange }\n                            values = { values }\n                        />\n                    </div>\n                )\n            case 3:\n                return (\n                    <div>\n                        <LinearProgress style={styles.progressBar} variant=\"determinate\" value={step * 100 / 3} />\n                        <Success />\n                    </div>\n                )\n        }\n        return (\n            <div></div>\n        )\n    }\n}\n\nconst styles = {\n    progressBar: {\n        height: 15,\n        marginBottom: 10\n    }\n}\n\nexport default TenantForm\n","/home/joaquin/Documents/Projects/leaning/home/react_step_form/src/components/FormTenantDetails.js",[],"/home/joaquin/Documents/Projects/leaning/home/react_step_form/src/components/Confirm.js",[],"/home/joaquin/Documents/Projects/leaning/home/react_step_form/src/components/Success.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":43,"column":9,"nodeType":"41","messageId":"42","endLine":74,"endColumn":10},"no-native-reassign",["43"],"no-negated-in-lhs",["44"],"default-case","Expected a default case.","SwitchStatement","missingDefaultCase","no-global-assign","no-unsafe-negation"]